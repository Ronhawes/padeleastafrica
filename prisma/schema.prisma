generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}



model Client {
  id String @id @defualt(uuid())
  name       String
  age        text
  imagePath  String
  createdAt             DateTime
  updatedAt             DateTime
}


